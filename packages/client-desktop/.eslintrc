{
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "node": true
  },
  "extends": [
    "eslint-config-airbnb",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "strict": "off",
    "no-bitwise": "off",
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": ["error", { "ignore": ["electron"] }]
  }
}
